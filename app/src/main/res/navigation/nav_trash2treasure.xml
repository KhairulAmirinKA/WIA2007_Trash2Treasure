        tools:layout="@layout/fragment_nearby_recycling_bin" />
    <fragment
        android:id="@+id/DestHWPS"
        android:name="com.techwizards.wia2007_trash2treasure.HouseholdWastePickupSchedule"
        android:label="fragment_household_waste_pickup_schedule"
        tools:layout="@layout/fragment_household_waste_pickup_schedule" />
    <fragment
        android:id="@+id/DestRecycled"
        android:name="com.techwizards.wia2007_trash2treasure.RecycledItemPickup"
        android:label="fragment_recycled_item_pickup"
        tools:layout="@layout/fragment_recycled_item_pickup" />
</navigation>