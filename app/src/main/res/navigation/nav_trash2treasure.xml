        android:name="com.techwizards.wia2007_trash2treasure.EditProfileFragment"
        android:label="fragment_edit_profile"
        tools:layout="@layout/fragment_edit_profile" />
    <fragment
        android:id="@+id/DestNotifications"
        android:name="com.techwizards.wia2007_trash2treasure.Notifications"
        android:label="fragment_notifications"
        tools:layout="@layout/fragment_notifications" />
    <fragment
        android:id="@+id/DestNearbyRecyclingBin"
